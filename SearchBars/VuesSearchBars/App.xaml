<Application x:Class="VuesSearchBars.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VuesSearchBars"
             StartupUri="SubscribeWindow.xaml">
    <Application.Resources>
        <Style x:Key="couleurBouton" TargetType="{x:Type Button}">
            <Setter Property="Foreground" Value="#808893" />
            <Setter Property="Background" Value="#B5E655"/>
        </Style>
    </Application.Resources>
</Application>
